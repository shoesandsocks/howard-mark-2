{
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "fetch": true
  },
  "rules": {
    "quotes": ["warn", "single"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": 0,
    "camelcase": 0,
    "arrow-parens": "off"
  }
}
